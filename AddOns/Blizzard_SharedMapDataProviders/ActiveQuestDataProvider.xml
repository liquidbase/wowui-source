<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\FrameXML\UI.xsd">
	<Script file="ActiveQuestDataProvider.lua"/>

	<Frame name="ActiveQuestPinTemplate" hidden="true" flattenRenderLayers="true" frameStrata="HIGH" enableMouseMotion="true" mixin="ActiveQuestPinMixin" virtual="true">
		<Size x="50" y="50"/>

		<Layers>
			<Layer level="ARTWORK">
				<Texture parentKey="Texture">
					<Anchors>
						<Anchor point="CENTER" />
					</Anchors>
				</Texture>
			</Layer>
			<Layer level="ARTWORK" textureSubLevel="1">
				<Texture parentKey="Number" file="Interface\WorldMap\UI-QuestPoi-NumberIcons">
					<Size x="32" y="32"/>
					<Anchors>
						<Anchor point="CENTER"/>
					</Anchors>
				</Texture>
			</Layer>
			<Layer level="HIGHLIGHT">
				<Texture parentKey="Highlight" alphaMode="ADD">
					<Anchors>
						<Anchor point="CENTER" />
					</Anchors>
				</Texture>
			</Layer>
		</Layers>
	</Frame>
</Ui>
